generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String 
  phone String @unique
  email String? @unique
  password String
  created_at DateTime @default(now())
  updated_at DateTime @default(now())
  deleted_at DateTime?


  @@map("users")
}